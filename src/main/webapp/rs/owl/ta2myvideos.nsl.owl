    <!-- http://www.ist-nm2.org/ontology/production#ID71a7ce077c524198b5e519a510c7ba1e -->

    <owl:NamedIndividual rdf:about="&production;ID71a7ce077c524198b5e519a510c7ba1e">
        <rdf:type rdf:resource="&core;TextualAnnotation"/>
        <key>terminationcondition</key>
        <!-- Updated for NSL1.1 -->
        <!-- <value>condition( length &gt;= ComposerDuration )</value> -->
        <value>sparql(
			PREFIX core: &lt;http://www.ist-nm2.org/ontology/core#&gt;
			PREFIX production: &lt;http://www.ist-nm2.org/ontology/production#&gt;
			PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt; 
			PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;
			PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;			
			ASK {
				{
					SELECT ( COUNT( ?audio ) AS ?audioCount )
					WHERE { ?audio core:hasBeenProcessed ?timstamp ; core:contains [ rdfs:label "Audio" ] }
				} .
				{
					?contextVariable a core:ContextVariable ;
						rdfs:label "ComposerDuration" ;
						core:hasValue ?composerDuration 
				} .
				FILTER( ?audioCount &lt; ?composerDuration )
			})
		</value>
	</owl:NamedIndividual>
    


